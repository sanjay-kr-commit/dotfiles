alias push='gitPass push sanjay'
alias hi='echo hi'
alias removeAndroidStudioLock='rm .var/app/com.google.AndroidStudio/config/Google/AndroidStudio2024.1/.lock'
alias xclip='xclip -selection c'
alias :q='exit'
alias tmux='/usr/bin/tmux -u'
alias :s='poweroff'
alias :r='reboot'
alias dotpush='currentDir=$(pwd) && cd $HOME/dotfiles && push && cd $currentDir && unset currentDir'
alias gitRemove='git rm --cached'
alias c+='zig c++'
alias updateList='apt list --upgradable| less'
alias ratemirror='rate-mirrors arch | sudo tee /etc/pacman.d/mirrorlist && rate-mirrors endeavouros | sudo tee /etc/pacman.d/endeavouros-mirrorlist'
alias ratemirrorandupdate='rate-mirrors arch | sudo tee /etc/pacman.d/mirrorlist && rate-mirrors endeavouros | sudo tee /etc/pacman.d/endeavouros-mirrorlist && eos-update --aur'
alias :i='nvim .'
alias removeOrphanPackages='sudo pacman -Rns $(pacman -Qdtq)'
alias removePackageCache='paccache -r && paccache -ruk0'
alias removeOldJournalEntries='journalctl --vacuum-time=4weeks'
alias updateMirror='reflector --protocol https --verbose --latest 25 --sort rate --save /etc/pacman.d/mirrorlist && eos-rankmirrors --verbose && eos-update --aur'
alias :si='sudo nvim .'
alias pull='git pull'
alias :c='clear'
alias :p='git pull'
alias c='zig cc -Wmain-return-type'
alias pythonPackageRebuild='yay -S $(yay -Qoq /usr/lib/"$(readlink /usr/bin/"$(readlink /usr/bin/python)" )") --answerclean All --noconfirm'
